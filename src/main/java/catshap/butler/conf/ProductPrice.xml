<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="productprice">

	<select id="selectProductPrice" parameterType="int" resultType="ProductPrice">
		select *
		from prodPrice
		where prodNo=#{prodNo}
	</select>
	
</mapper>
